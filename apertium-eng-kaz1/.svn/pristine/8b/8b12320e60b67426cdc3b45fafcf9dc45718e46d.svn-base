    <def-macro n="reset-case1" npar="1">
          <let><var n="PREP"/><lit v=""/></let>
    </def-macro>
<def-macro n="reset-cond1" npar="1">
          <let><var n="IF"/><lit v=""/></let>
    </def-macro>

    <def-macro n="set-prep1" npar="1"> 
          <let><var n="PREP"/><lit v=""/></let>
          <choose> 
            <when>
               <test> 
                  <equal><clip pos="1" part="a_sn_type"/><lit-tag v="SN.DAT"/></equal>
               </test>
               <let><var n="PREP"/>
                 <concat>
                  <get-case-from pos="1">
                    <lit v="^prep"/>
                  </get-case-from>
                  <lit-tag  v="pr"/>
                  <lit v="{"/>
                  <lit v="^to"/>
                  <lit-tag  v="pr"/>
                  <lit v="$}$ "/>
                </concat>
               </let>
            </when> 

            <when>
               <test> 
                  <equal><clip pos="1" part="a_sn_type"/><lit-tag v="SN.LOC"/></equal>
               </test>
               <let><var n="PREP"/>
                 <concat>
                  <get-case-from pos="1">
                    <lit v="^prep"/>
                  </get-case-from>
                  <lit-tag  v="pr"/>
                  <lit v="{"/>
                  <lit v="^in"/>
                  <lit-tag  v="pr"/>
                  <lit v="$}$ "/>
                </concat>
               </let>
            </when> 
            <when>
               <test> 
                  <equal><clip pos="1" part="a_sn_type"/><lit-tag v="SN.INS"/></equal>
               </test>
               <let><var n="PREP"/>
                 <concat>
                  <get-case-from pos="1">
                    <lit v="^prep"/>
                  </get-case-from>
                  <lit-tag  v="pr"/>
                  <lit v="{"/>
                  <lit v="^with"/>
                  <lit-tag  v="pr"/>
                  <lit v="$}$ "/>
                </concat>
               </let>
            </when> 
            <when>
               <test> 
                  <equal><clip pos="1" part="a_sn_type"/><lit-tag v="SN.ABL"/></equal>
               </test>
               <let><var n="PREP"/>
                 <concat>
                  <get-case-from pos="1">
                    <lit v="^prep"/>
                  </get-case-from>
                  <lit-tag  v="pr"/>
                  <lit v="{"/>
                  <lit v="^from"/>
                  <lit-tag  v="pr"/>
                  <lit v="$}$ "/>
                </concat>
               </let>
            </when> 

          </choose>
    </def-macro>
  </section-def-macros>
