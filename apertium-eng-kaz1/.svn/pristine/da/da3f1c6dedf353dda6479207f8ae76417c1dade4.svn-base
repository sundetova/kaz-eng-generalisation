<def-macro n="NEW-build-noun-lu-2-4-5" npar="1"> 
     <let>
           <var n="NEW-noun_lu"/>
           <concat>
               <clip pos="1" side="tl" part="lem"/> 
               <clip pos="1" side="tl" part="a_nom"/>
               <lit-tag v="2"/> 
		<lit-tag v="3"/> 
               <lit-tag v="4"/> <!-- for possessive -->
             <!--  <lit-tag v="5"/>--> 
