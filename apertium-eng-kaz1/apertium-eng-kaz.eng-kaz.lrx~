<rule>
    <match lemma="the" tags="det.*"/>
    <match lemma="time" tags="n.*"><select lemma="уақыт" tags="n.*"/></match>
    <match lemma="*" tags="num.*"/>  
     </rule>

<rule>
    <match lemma="what" tags="adv.*"/>
