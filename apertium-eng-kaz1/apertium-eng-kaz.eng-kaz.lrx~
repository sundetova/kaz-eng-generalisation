<rules>
  <rule> <!-- to solve something that CG couldn't? -->
    <match lemma="I" tags="prn.*"><select lemma="Мен" tags="prn.*"/></match>
  </rule>
  <rule> <!-- leftover example -->
    <match lemma="dog" tags="n.*"><select lemma="ит" tags="n.*"/></match>
  </rule>

  <rule> <!-- probably does not belong here? -->
    <match lemma="residence" tags="n.*"><select lemma="мекен" tags="n.*"/></match>
  </rule>

  <rule>
    <match lemma="state" tags="n.*"><select lemma="мемлекет" tags="n.*"/></match>
  </rule>

<rule>
    <match lemma="ceremony" tags="n.*"><select lemma="салтанат" tags="n.*"/></match>
  </rule>

  <rule>
    <match lemma="number" tags="n.*"><select lemma="нөмір" tags="n.*"/></match>
    <match lemma="*" tags="num.*"/>
   
  </rule>

  <rule>
    <match lemma="number" tags="n.*"><select lemma="сан" tags="n.*"/></match>
  </rule>
<rule>
    <match lemma="home" tags="n.*"><select lemma="үй" tags="n.*"/></match>
  </rule>

<rule>
    <match lemma="call" tags="vblex.*"><select lemma="ата" tags="v.*"/></match>
  </rule>

<rule>
    <match lemma="through" tags="pr"><select lemma="арқылы" tags="post.Rnom"/></match>
  </rule>

  <rule>
    <match lemma="residence" tags="n.*"><select lemma="резиденция" tags="n.*"/></match>
    <match lemma="of" tags="pr"/>
    <match lemma="the" tags="det.*"/>
    <match lemma="president" tags="n.*"/>
  </rule>



  <rule>
 <match lemma="have" tags="vbhaver.*"/>
    <match lemma="be" tags="vbser.pp"><select lemma="бол" tags="v.*"/></match>
    
  </rule>

  <rule>

    <match lemma="be" tags="vbser.pp"><select lemma="бол" tags="v.*"/></match>
    
  </rule>

<rule>
    <match lemma="boot" tags="n.*"><select lemma="жүксалғыш" tags="n.*"/></match>
    <match lemma="of" tags="pr"/>
    <match lemma="the" tags="det.*"/>
    <match lemma="car" tags="n.*"/>
  </rule>


  <rule>
    <match lemma="government" tags="n.*"/>
    <match lemma="residence" tags="n.*"><select lemma="резиденция" tags="n.*"/></match>
  </rule>

 

 <rule> 
  
   
