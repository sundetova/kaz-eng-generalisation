<rule>
    <match lemma="that" tags="det.*"><select lemma="осы" tags="det.*"/></match>
    <match lemma="time" tags="n.*"><select lemma="уақыт" tags="n.*"/></match>
     </rule>

  <rule> <!-- default translation -->
    <match lemma="possession" tags="n.pl"><select lemma="дүние" tags="n.*"/></match>
  </rule>

  <rule>  <!-- not completely sure about this -->
    <match lemma="in" tags="pr"/>  
